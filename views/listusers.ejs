<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  type="text/css" href="main.css" />
    <title>List users of <%=shopName%></title>
  </head>
  <body>
    <h1>List users of <%=shopName%></h1>
    <table>
      <tr>
        <th>First Name</th>
        <th>Surname</th>
        <th>Email</th>
        <th>Username</th>
      </tr>
      <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.first %></td>
          <td><%= user.last %></td>
          <td><%= user.email %></td>
          <td><%= user.username%></td>
        </tr>  
      <% }) %>
    </table>
  <button><a href="/">Home page</a></button>
  </body>  
</html>